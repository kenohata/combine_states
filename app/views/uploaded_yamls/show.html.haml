.container-fluid
  .row.page-break-after-always
    = form_for @yaml, method: :put, html: { class: "form-inline" } do |f|
      .form-group
        = f.label :file
        = f.file_field :file
      = f.submit class: "btn btn-default"
  - @tree.each_with_index do |state, i|
    .panel.panel-default.page-break-inside-avoid
      .panel-heading #{i + 1}.
      %table.table.table-condensed.table-striped
        %tr
          - state.each_with_index do |val, index|
            %th= @tree.keys[index]
        %tr
          - state.each_with_index do |val, index|
            %td= val
